								</VisualState>
								<VisualState x:Name="Disabled"/>
							</VisualStateGroup>
						</VisualStateManager.VisualStateGroups>
						<Grid x:Name="正常" Height="16">
							<Rectangle Margin="4,0.002,0,2.998" Stroke="White" StrokeThickness="2" Height="13" Fill="{x:Null}" Opacity="0.7" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
							<Rectangle Margin="0,3.616,4,-0.616" Stroke="White" StrokeThickness="2" Height="13" Fill="{x:Null}" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
						</Grid>
						<Grid x:Name="移动" Height="16">
							<Rectangle Margin="4,0.002,0,2.998" Stroke="#FF2070BA" StrokeThickness="2" Height="13" Fill="{x:Null}" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
							<Rectangle Margin="0,3.616,4,-0.616" Stroke="#FF2070BA" StrokeThickness="2" Height="13" Fill="{x:Null}" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
						</Grid>
						<Grid x:Name="点击" Height="16" >
							<Rectangle Margin="4,0.002,0,2.998" Stroke="#FF0A4276" StrokeThickness="2" Height="13" Fill="{x:Null}" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
							<Rectangle Margin="0,3.616,4,-0.616" Stroke="#FF0A4276" StrokeThickness="2" Height="13" Fill="{x:Null}" >
								<Rectangle.Effect>
									<DropShadowEffect Color="#FF2C6FB2" BlurRadius="2" ShadowDepth="1" Opacity="0.5"/>
								</Rectangle.Effect>
							</Rectangle>
						</Grid>
                        
						<Rectangle Margin="0,-10,0,-10" Fill="White" Opacity="0"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
		<Setter Property="Focusable" Value="False"/>
